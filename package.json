{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "release": "turbo run build && changeset publish",
    "clean": "turbo run clean",
    "dev": "turbo run dev --parallel",
    "generate:index": "npx ts-node scripts/generateDocsIndex.ts",
    "generate:readme": "npx ts-node scripts/generateReadme.ts",
    "lint": "turbo run lint",
    "new:package": "npx ts-node scripts/newPackage.ts",
    "postinstall": "husky install",
    "publish-packages": "yarn build && lerna publish from-package --yes",
    "start": "turbo run start --parallel",
    "test": "jest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@testing-library/dom": "^7.26.3",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^26.0.15",
    "@types/listr": "^0.14.4",
    "@types/node": "^13.1.1",
    "@types/react-dom": "latest",
    "@vercel/ncc": "^0.33.1",
    "chalk": "^4.1.2",
    "husky": "^7.0.4",
    "inquirer": "^8.1.2",
    "jest-junit": "^12.0.0",
    "jest": "^26.6.1",
    "listr": "^0.14.3",
    "react-dom": "latest",
    "react-test-renderer": "latest",
    "react": "latest",
    "regenerator-runtime": "^0.13.9",
    "ts-jest": "^26.4.3",
    "tsc-watch": "^4.2.9",
    "turbo": "^1.8.5",
    "typescript": "^4.5.4"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "shared/*"
  ]
}
