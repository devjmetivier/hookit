{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean:workspaces": "turbo clean",
    "clean": "rm -rf node_modules",
    "dev": "turbo run dev --parallel",
    "generate:index": "pnpm dlx tsx scripts/generateDocsIndex.ts",
    "generate:readme": "pnpm dlx tsx scripts/generateReadme.ts",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "new:package": "pnpm dlx tsx scripts/newPackage.ts",
    "publish-packages": "pnpm build && lerna publish from-package --yes",
    "release": "turbo run build && changeset publish",
    "start": "turbo run start --parallel",
    "test": "jest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@hookit-local/tsconfig": "*",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.0",
    "@types/listr": "^0.14.4",
    "@types/node": "^18.15.11",
    "@types/react-dom": "latest",
    "@vercel/ncc": "^0.36.1",
    "chalk": "^5.2.0",
    "eslint-config-devjmetivier": "^2.5.3",
    "inquirer": "^9.1.5",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "listr": "^0.14.3",
    "react": "latest",
    "react-dom": "latest",
    "react-test-renderer": "latest",
    "regenerator-runtime": "^0.13.11",
    "ts-jest": "^29.1.0",
    "tsc-watch": "^6.0.0",
    "turbo": "^1.8.8",
    "typescript": "^5.0.4"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "shared/*"
  ]
}
